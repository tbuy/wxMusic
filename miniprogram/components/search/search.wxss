@import "../../iconfont.wxss";
.search {
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.iconfont {
  font-size: 50rpx;
  margin-top: 15rpx;
  
}
.searchIcon {
  font-size: 36rpx;
  margin-top: 12rpx;
  color: rgb(151, 151, 151);
  margin-left: 20rpx;
}
.searchInput {
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
  width: 500rpx;
  background-color: #eeeeee;
  border-radius: 60rpx;
  overflow: hidden;
}
.input {
  width: 400rpx;
  height: 40rpx;
  font-size: 30rpx;
  float: right;
  margin-top: 8rpx;
  margin-right: 20rpx;
}
.searchBtn {
  font-size: 28rpx;
  color: #fff;
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 10rpx;
  margin-top: 15rpx;
  background-color: #d43c33;
  text-align: center;
}